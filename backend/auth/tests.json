{
  "tests": [
    {
      "name": "Register existing user returns error",
      "method": "POST",
      "body": {
        "action": "register",
        "username": "testuser",
        "password": "test123",
        "bio": "Test user"
      },
      "expectedStatus": 409,
      "expectedBody": {
        "error": "Username already exists"
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "Login existing user",
      "method": "POST",
      "body": {
        "action": "login",
        "username": "testuser",
        "password": "test123"
      },
      "expectedStatus": 200,
      "expectedBody": {
        "success": true
      },
      "bodyMatcher": "partial"
    }
  ]
}